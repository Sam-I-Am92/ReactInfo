{
  "name": "reactapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --coverage --colors --watchAll",
    "start": "nodemon ./server/start.js",
    "react-dev": "webpack --watch"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "author": "Parteek Sandhu",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "lowdb": "^3.0.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "octokit": "^2.0.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.4.0",
    "react-router-dom": "^6.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.8",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@faker-js/faker": "^7.3.0",
    "@redux-devtools/core": "^3.13.1",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "jest": "^28.1.2",
    "knex": "^2.1.0",
    "nodemon": "^2.0.18",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.2.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
